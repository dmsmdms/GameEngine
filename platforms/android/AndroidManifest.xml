<?xml version="1.0"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionName="$CONFIG_PROJECT_VERSION_FULL_STR"
	android:versionCode="$CONFIG_PROJECT_VERSION_FULL"
	package="$CONFIG_PROJECT_PACKAGE">
	<uses-sdk android:minSdkVersion="$CONFIG_ANDROID_SDK_VERSION"
		android:targetSdkVersion="$CONFIG_ANDROID_TARGET_PLATFORM"/>
	<application android:label="$CONFIG_PROJECT_NAME">
		<activity android:name="$CONFIG_PROJECT_PACKAGE.MainActivity"
			android:label="$CONFIG_PROJECT_NAME">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
	</application>
</manifest>
